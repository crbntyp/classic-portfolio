# Database Configuration
# Copy this file to .env and fill in your actual credentials

# Docker/Development Database (connects to remote production DB)
DB_DOCKER_HOST=your_production_ip_here
DB_DOCKER_USER=your_database_user
DB_DOCKER_PASS=your_database_password
DB_DOCKER_NAME=your_database_name

# Production Database
DB_PROD_HOST=localhost
DB_PROD_USER=your_database_user
DB_PROD_PASS=your_database_password
DB_PROD_NAME=your_database_name
